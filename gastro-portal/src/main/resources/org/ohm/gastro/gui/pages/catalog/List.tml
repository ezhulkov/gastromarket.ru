<t:htmllayout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" class="starter-template">

    <div class="main-body">

        <t:filter category="category"/>

        <ul>
            <t:loop source="products" value="oneProduct" formState="NONE">
                <li>
                    <t:product product="oneProduct"/>
                </li>
            </t:loop>
        </ul>

    </div>

</t:htmllayout>
