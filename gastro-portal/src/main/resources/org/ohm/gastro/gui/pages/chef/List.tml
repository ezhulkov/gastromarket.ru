<t:htmllayout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" class="starter-template">

    <div class="main-body">
        <h1>Кулинары</h1>

        <ul>
            <t:loop source="catalogs" value="oneCatalog" formState="NONE">
                <li>
                    <t:pagelink page="chef" context="oneCatalog.id">${oneCatalog.name}</t:pagelink>
                </li>
            </t:loop>
        </ul>

    </div>

</t:htmllayout>
